RM=rm -f

all: vnccrack

OBJS=vnccrack.o

CFLAGS = -O2 -Wall -W @BOTAN_CPPFLAGS@

vnccrack: $(OBJS)
	@CXX@ $(OBJS) @BOTAN_LIBS@ -lpcap -o vnccrack

vnccrack.o: vnccrack.cpp
	@CXX@ $(CFLAGS) -c $< -o $@

clean:
	$(RM) vnccrack *.o
	$(RM) config.status config.log
	$(RM) -r autom4te*.cache

distclean: clean
	$(RM) Makefile
